{
    "transform": {
        "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "globals": {
        "ts-jest": {
            "tsConfig": "jest.tsconfig.json"
        }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "node"
    ]
}
